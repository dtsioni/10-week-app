%div{id:"piccont"}
  %a{href:home_path}
    %img{:src => "http://bizitvideo.com/wp-content/uploads/2013/11/welcome.png",
    :style => "display:block;margin-left:auto;margin-right:auto;width:200px;heigth:auto;filter:alpha(opacity=100);"}
  %p{:id => "notice"}
    =notice
  -if signed_in?
    =link_to "Logged In As: #{current_user.username}", current_user
    =link_to "Sign Out", signout_path, method: "delete"
  -else
    %p Not signed in!

